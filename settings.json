{
    "workbench.editor.showTabs": "single",
    "window.zoomLevel": 1,
    "window.commandCenter": false,
    "editor.fontSize": 16,
    // "http.proxyAuthorization": null,
    // "http.proxy": "http://",
    "telemetry.telemetryLevel": "off",
    "files.autoSave": "afterDelay",
    "editor.fontFamily": "Jetbrains Mono, Menlo, Monaco, 'Courier New', monospace",
    "editor.lineNumbers": "relative",
    "editor.lineHeight": 1.7,
    "editor.stickyScroll.enabled": false,
    "editor.linkedEditing": true,
    "explorer.confirmDelete": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "typescript.updateImportsOnPaste.enabled": true,
    "workbench.sideBar.location": "right",
    "editor.fontLigatures": true,
    "workbench.settings.applyToAllProfiles": [],
    "vim.foldfix": true,
    "vim.hlsearch": true,
    "vim.highlightedyank.enable": true,
    "vim.highlightedyank.color": "rgba(250, 240, 170, 0.7)",
    "vim.highlightedyank.textColor": "white",
    "vim.enableNeovim": true,
    "vim.neovimUseConfigFile": true,
    "vim.neovimConfigPath": "~/.config/nvim/init.vim",
    "vim.leader": "<Space>",
    "vim.visualModeKeyBindings": [
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        },
        {
            "before": [
                "ctrl+h"
            ],
            "commands": [
                "editor.action.moveLinesDownAction"
            ]
        },
        {
            "before": [
                "ctrl+j"
            ],
            "commands": [
                "editor.action.moveLinesUpAction"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "$"
            ],
            "after": [
                "g",
                "_"
            ]
        },
        {
            "before": [
                "g",
                "_"
            ],
            "after": [
                "$"
            ]
        }
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "leader",
                "l",
                "s"
            ],
            "commands": [
                "workbench.action.gotoSymbol"
            ]
        },
        {
            "before": [
                "leader",
                "l",
                "p"
            ],
            "commands": [
                "workbench.action.showAllSymbols"
            ]
        },
        {
            "before": [
                "leader",
                "l",
                "r"
            ],
            "commands": [
                "editor.action.rename"
            ]
        },
        {
            "before": [
                "leader",
                "l",
                "c"
            ],
            "commands": [
                "editor.action.quickFix"
            ]
        },
        {
            "before": [
                "ctrl+h"
            ],
            "commands": [
                "editor.action.moveLinesDownAction"
            ]
        },
        {
            "before": [
                "ctrl+j"
            ],
            "commands": [
                "editor.action.moveLinesUpAction"
            ]
        },
        {
            "before": [
                "leader",
                "v"
            ],
            "commands": [
                ":vsplit"
            ]
        },
        {
            "before": [
                "leader",
                "s"
            ],
            "commands": [
                ":split"
            ]
        },
        {
            "before": [
                "leader",
                "n",
                "h"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "leader",
                "e"
            ],
            "commands": [
                "editor.action.format",
                "workbench.action.files.save"
            ]
        },
        {
            "before": [
                "g",
                "p",
                "d"
            ],
            "commands": [
                "editor.action.peekDefinition"
            ]
        },
        {
            "before": [
                "g",
                "p",
                "t"
            ],
            "commands": [
                "editor.action.peekTypeDefinition"
            ]
        },
        {
            "before": [
                "g",
                "p",
                "i"
            ],
            "commands": [
                "editor.action.peekImplementation"
            ]
        },
        {
            "before": [
                "shift+k"
            ],
            "commands": [
                "editor.action.showDefinitionPreviewHover"
            ]
        },
        {
            "before": [
                "g",
                "i"
            ],
            "commands": [
                "editor.action.goToImplementation"
            ]
        },
        {
            "before": [
                "g",
                "r"
            ],
            "commands": [
                "editor.action.referenceSearch.trigger"
            ]
        },
        {
            "before": [
                "g",
                "t"
            ],
            "commands": [
                "editor.action.goToTypeDefinition"
            ]
        },
        {
            "before": [
                "leader",
                "p"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "leader",
                "h"
            ],
            "commands": [
                "workbench.action.focusLeftGroup"
            ]
        },
        {
            "before": [
                "leader",
                "j"
            ],
            "commands": [
                "workbench.action.focusBelowGroup"
            ]
        },
        {
            "before": [
                "leader",
                "k"
            ],
            "commands": [
                "workbench.action.focusAboveGroup"
            ]
        },
        {
            "before": [
                "leader",
                "l"
            ],
            "commands": [
                "workbench.action.focusRightGroup"
            ]
        }
    ],
    "editor.minimap.enabled": false,
    "git.autofetch": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.renderSideBySide": true,
    "cursor.general.gitGraphIndexing": "enabled",
    "cursor-retrieval.canAttemptGithubLogin": false,
    "[php]": {
        "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
    },
    "php-cs-fixer.executablePath": "~/.composer/vendor/bin/phpcs",
    "php-cs-fixer.rules": "@PSR12",
    "gitlens.currentLine.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.plusFeatures.enabled": false,
    "gitlens.codeLens.authors.enabled": false,
    "projectManager.git.baseFolders": [
        "~/Documents/"
    ],
    "projectManager.filterOnFullPath": true,
    "projectManager.git.maxDepthRecursion": 3,
    "projectManager.hg.maxDepthRecursion": 3
}
